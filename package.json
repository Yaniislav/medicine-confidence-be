{
  "name": "medicine-confidence-be",
  "version": "0.0.1",
  "description": "Medicine Confidence",
  "main": "src/index.js",
  "engines": {
    "node": ">=10.3"
  },
  "scripts": {
    "babel-node": "babel-node",
    "build": "NODE_ENV=production babel src --out-dir dist --source-maps inline",
    "pull": "rm -rf node_modules && rm -f package-lock.json && git pull origin master && npm i",
    "start": "nodemon --watch src --exec babel-node src/index.js",
    "prod": "NODE_ENV=prodaction node app.js",
    "doc": "apidoc -i 'src/routeIn' -o apidoc/",
    "compose": "docker-compose up"
  },
  "dependencies": {
    "apidoc": "^0.17.7",
    "bluebird": "^3.7.1",
    "dotenv": "^8.2.0",
    "fastest-validator": "^0.6.12",
    "joi": "^14.3.1",
    "koa": "^2.5.1",
    "koa-body": "^2.5.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.15",
    "moment": "^2.20.1",
    "mongoose": "^5.7.12",
    "path": "^0.12.7",
    "q": "^1.5.1",
    "redis": "^2.8.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-core": "^6.26.3",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-react": "^7.6.1",
    "nodemon": "^1.15.0"
  }
}
